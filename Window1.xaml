<Window x:Class="WpfApp1_ИСРПО_Гуляева_402.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1_ИСРПО_Гуляева_402"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <ListBox Margin="10 10 0 13" Name="listBox1" HorizontalAlignment="Left" VerticalAlignment="Top"  Width="194" Height="200">
            <ListBoxItem Content="Coffie"></ListBoxItem>
            <ListBoxItem Content="Tea"></ListBoxItem>
            <ListBoxItem Content="Organge Juice"></ListBoxItem>
            <ListBoxItem Content="Milk"></ListBoxItem>
            <ListBoxItem Content="Iced Tea"></ListBoxItem>
            <ListBoxItem Content="Mango Shake"></ListBoxItem>
            <Button Height="23" x:Name="button1"
                VerticalAlignment="Top"
                HorizontalAlignment="Left"
                Width="76"
                Click="button1_Click" Content="Add Item"/>
        </ListBox>

        <TextBox Height="23"
                 HorizontalAlignment="Left"
                 Margin="8 14 0 0"
                 Name="textBox1"
                 VerticalAlignment="Top" Width="193"/>
    </Grid>
</Window>
